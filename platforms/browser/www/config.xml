<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phidias.app.test" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Phidias Test</name>
    <description>
        Phidias test application
    </description>
    <author email="support@phidias.com" href="http://phidias.com">
        Phidias SAS
    </author>
    <content src="index.html" />
    <icon src="resources/android/icon/drawable-xhdpi-icon.png" />
    <splash src="resources/android/splash/drawable-port-xhdpi-screen.png" />
    <access origin="*" subdomains="true" uri="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
